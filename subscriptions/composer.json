{
    "name": "ijideals/subscriptions",
    "description": "Package for managing subscriptions using lucasdotvin/laravel-soulbscription for IJIDeals.",
    "type": "laravel-package",
    "license": "proprietary",
    "authors": [
        {
            "name": "IJIDeals",
            "email": "contact@ijideals.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "lucasdotvin/laravel-soulbscription": "^3.0",
        "ijideals/user-management": "*",
        "ijideals/payments": "*"
    },
    "autoload": {
        "psr-4": {
            "IJIDeals\\Subscriptions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IJIDeals\\Subscriptions\\Providers\\SubscriptionServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

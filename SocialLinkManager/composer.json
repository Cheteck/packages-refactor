{
    "name": "ijideals/sociallinkmanager",
    "description": "Flexible social link management for Laravel Eloquent models with support for configurable platforms and feature gating.",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "IJIDeals",
            "email": "contact@ijideals.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/validation": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "IJIDeals\\SocialLinkManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IJIDeals\\SocialLinkManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IJIDeals\\SocialLinkManager\\SocialLinkManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

{
    "name": "ijideals/ijicommerce",
    "description": "Laravel package for shop management with team roles, designed for multi-vendor e-commerce.",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "IJIDeals",
            "email": "contact@ijideals.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/routing": "^9.0|^10.0|^11.0",
        "spatie/laravel-permission": "^5.5|^6.0",
        "ijideals/ijiproductcatalog": "*",
        "ijideals/ijishoplistings": "*",
        "ijideals/ijiordermanagement": "*"
    },
    "autoload": {
        "psr-4": {
            "IJIDeals\\IJICommerce\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IJIDeals\\IJICommerce\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IJIDeals\\IJICommerce\\IJICommerceServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

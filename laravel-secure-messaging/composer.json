{
    "name": "ijideals/laravel-secure-messaging",
    "description": "A Laravel package for secure end-to-end encrypted messaging.",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Jules AI",
            "email": "jules@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "ext-sodium": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Acme\\SecureMessaging\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acme\\SecureMessaging\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Acme\\SecureMessaging\\SecureMessagingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

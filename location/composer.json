{
    "name": "ijideals/location",
    "description": "Location management package for IJIDeals applications, including models for countries, cities, and related functionalities.",
    "keywords": ["laravel", "package", "location", "country", "city"],
    "homepage": "https://github.com/ijideals/location",
    "license": "proprietary",
    "type": "laravel-package",
    "authors": [
        {
            "name": "IJIDeals",
            "email": "contact@ijideals.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "astrotomic/laravel-translatable": "^11.0",
        "cviebrock/eloquent-sluggable": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0",
        "pestphp/pest": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "IJIDeals\\Location\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IJIDeals\\Location\\Providers\\LocationServiceProvider"
            ],
            "aliases": {
                "Location": "IJIDeals\\Location\\Facades\\Location"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

{
    "name": "ijideals/recommendation-engine",
    "description": "Provides product recommendation services for the IJIDeals platform.",
    "type": "laravel-package",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "IJIDeals\\RecommendationEngine\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IJIDeals\\RecommendationEngine\\RecommendationEngineServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/database": "^10.0",
        "ijideals/analytics": "*",
        "ijideals/ijiproductcatalog": "*",
        "ijideals/user-management": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
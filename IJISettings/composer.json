{
    "name": "ijideals/ijisettings",
    "description": "Manages platform-wide settings for the IJIDeals application.",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "IJIDeals Team",
            "email": "dev@ijideals.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/cache": "^9.0|^10.0|^11.0",
        "illuminate/encryption": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "IJIDeals\\IJISettings\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IJIDeals\\IJISettings\\Providers\\IJISettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

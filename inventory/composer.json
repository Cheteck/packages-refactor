    "description": "Inventory management package for IJIDeals platform",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "IJIDeals\\Inventory\\": "src/"
        }
    },
    "authors": [
        {
            "name": "IJIDeals",
            "email": "contact@ijideals.com"
        }

{
    "name": "ijideals/ijishoplistings",
    "description": "Laravel package for managing shop-specific product listings.",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "IJIDeals",
            "email": "contact@ijideals.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/routing": "^9.0|^10.0|^11.0",
        "ijideals/ijicommerce": "*",
        "ijideals/ijiproductcatalog": "*",
        "spatie/laravel-medialibrary": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "IJIDeals\\IJIShopListings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IJIDeals\\IJIShopListings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IJIDeals\\IJIShopListings\\IJIShopListingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
